generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/core"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_CORE")
}

model Barcode {
  id        String   @id @default(uuid())
  value     String   @unique
  format    String
  createdAt DateTime @default(now())
}

model Product {
  id        String   @id @default(uuid())
  name      String
  barcode   String   @unique
  price     Float?
  createdAt DateTime @default(now())
}
